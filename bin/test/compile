#!/bin/bash
gcc -gstabs+ -Wall -DDBG types.c -o types
gcc -gstabs+ -Wall -DDBG hello.c -o hello
gcc -gstabs+ -Wall -DDBG int3.c -o int3
gcc -gstabs+ -Wall -DDBG app12.c app22.c -o app

gcc -gstabs+ -Wall -I/lib/modules/`uname -r`/build/include -D__KERNEL__ -D_LOOSE_KERNEL_NAMES -DLINUX -DDBG -c module.c
gcc -gstabs+ -Wall -I/lib/modules/`uname -r`/build/include -D__KERNEL__ -D_LOOSE_KERNEL_NAMES -DLINUX -DDBG -c modlp.c
gcc -gstabs+ -Wall -I/lib/modules/`uname -r`/build/include -D__KERNEL__ -D_LOOSE_KERNEL_NAMES -DLINUX -DDBG -c modscope.c
